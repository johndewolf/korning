<h1>Customers</h1>
<table>
  <th>Customer ID</th>
  <th>Customer Name</th>
  <th>Customer Account Number</th>

  <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id%></td>
      <td><a href="http://www.<%= customer.name%>.com"><%= customer.name %></a></td>
      <td><%= customer.account_number %></td>
    </tr>
  <% end%>
</table>

<h1>Employees</h1>
<table>
  <th>Employee ID</th>
  <th>Last Name</th>
  <th>First Name</th>
  <th>Email</th>
  <% @employees.each do |employee| %>
    <tr>
      <td><%= employee.id%></td>
      <td><a href="/employees/<%= employee.id%>/"><%= employee.last_name %></a></td>
      <td><%= employee.first_name %></td>
      <td><a href="mailto:<%= employee.email %>"><%= employee.email %></td>
    </tr>
  <% end%>
</table>
